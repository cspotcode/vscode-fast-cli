{
    "name": "vscode-faster-editor-launch",
    "displayName": "vscode-faster-editor-launch",
    "description": "Faster CLI command to open files in VSCode.",
    "version": "0.0.1",
    "publisher": "cspotcode",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "bin": {
        "fcode": "./out/fcode-cli.js"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "typescript": "^3.1.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "@types/cross-spawn": "^6.0.0",
        "@types/lodash": "^4.14.116",
        "cross-spawn": "^6.0.5",
        "lodash.escaperegexp": "^4.1.2"
    }
}
