{
    "name": "vscode-fast-cli",
    "displayName": "vscode-fast-cli",
    "description": "CLI command to open files in VSCode faster than built-in CLI on Windows.",
    "version": "0.0.1",
    "publisher": "cspotcode",
    "os": [
        "win32"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cspotcode/vscode-fast-cli.git"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "prepare": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/cross-spawn": "^6.0.0",
        "@types/lodash": "^4.14.116",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "typescript": "^3.1.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "cross-spawn": "^6.0.5",
        "lodash.escaperegexp": "^4.1.2"
    },
    "bin": {
        "fcode": "out/fcode-cli.js"
    },
    "files": [
        "out",
        "src",
        "bring-to-front"
    ]
}
